    "build": "rimraf dist && openapi-ts && tsc",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "0.90.1",
    "@types/node": "^24.1.0",
    "drizzle-kit": "^1.0.0-beta.14-a36c63d",
    "rimraf": "^6.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@libsql/client": "^0.17.0",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^1.0.0-beta.14-a36c63d"
  }
}